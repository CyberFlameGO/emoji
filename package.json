{
  "name": "@jetbrains/emoji",
  "version": "1.0.3",
  "description": "JetBrains Emoji Library",
  "files": [
    "dirname.js",
    "*.svg"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:JetBrains/emoji.git"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "ci-publish": "^1.3.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "standard-version": "^4.0.0",
    "svgo": "^0.7.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "scripts": {
    "commit": "git-cz",
    "prerelease:ci": "svgo -f src -o .",
    "release:ci": "standard-version && git push --follow-tags origin master && ci-publish"
  }
}
